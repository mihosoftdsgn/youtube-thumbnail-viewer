{
  "name": "YouTube Thumbnail Viewer",
  "version": "1.2.0",
  "icons": {
    "128": "128.png"
  },
  "manifest_version": 3,
  "description": "YouTube Thumbnail Viewer",
  "host_permissions": [
    "https://www.youtube.com/*"
  ],
  "content_scripts": [{
    "matches": [
      "https://www.youtube.com/watch*"
    ],
    "js": [
      "content.js"
    ]
  }]
}